<AnchorPane prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.commander.app.WizController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <VBox BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="header" alignment="CENTER" contentDisplay="CENTER" prefHeight="0.0" prefWidth="500.0" text="Task Wizard">
                     <font>
                        <Font name="Tw Cen MT Condensed" size="64.0" />
                     </font>
                  </Label>
                  <Label fx:id="subheader" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="0.0" prefWidth="500.0" text="subheader" textAlignment="RIGHT">
                     <font>
                        <Font name="Tw Cen MT Condensed" size="36.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </top>
         <center>
            <Pane prefHeight="429.0" prefWidth="500.0" BorderPane.alignment="CENTER">
               <children>
                  <Label alignment="CENTER" contentDisplay="TOP" layoutY="108.0" prefHeight="25.0" prefWidth="500.0" text="Name to give to new workbook:" />
                  <Label alignment="CENTER" contentDisplay="CENTER" layoutY="227.0" prefHeight="25.0" prefWidth="500.0" text="Name to give to new spreadsheet:" />
                  <TextField fx:id="textField1" layoutX="150.0" layoutY="133.0" />
                  <TextField fx:id="textField2" layoutX="150.0" layoutY="252.0" />
               </children>
            </Pane>
         </center>
         <bottom>
            <HBox prefHeight="68.0" prefWidth="500.0" BorderPane.alignment="CENTER">
               <children>
                  <Pane prefHeight="68.0" prefWidth="50.0" />
                  <Button mnemonicParsing="false" onAction="#handleCancelTaskWiz" prefHeight="37.0" prefWidth="91.0" text="CANCEL" />
                  <Pane prefHeight="68.0" prefWidth="229.0" />
                  <Button mnemonicParsing="false" onAction="#handleSubmitNewWB" prefHeight="37.0" prefWidth="92.0" text="SUBMIT" />
               </children>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
