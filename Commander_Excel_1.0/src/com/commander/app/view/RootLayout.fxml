<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.commander.app.MainMenuController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="800.0">
         <left>
            <VBox prefHeight="563.0" prefWidth="206.0" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <TableView prefHeight="600.0" prefWidth="253.0" BorderPane.alignment="CENTER">
              <columns>
                  <TableColumn prefWidth="253.0" text="Tasks" />
              </columns>
            </TableView>
         </right>
         <center>
            <VBox BorderPane.alignment="CENTER">
               <children>
                  <Pane prefHeight="203.0" prefWidth="341.0" />
                  <HBox>
                     <children>
                        <Pane prefHeight="0.0" prefWidth="70.0" />
                        <Button alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#handleCreateProjectAction" text="Create New Project">
                           <font>
                              <Font name="System Bold" size="16.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
                  <Separator opacity="0.0" prefHeight="0.0" prefWidth="173.0" />
                  <Separator opacity="0.0" prefHeight="18.0" prefWidth="341.0" />
                  <HBox>
                     <children>
                        <HBox>
                           <children>
                              <Pane prefHeight="32.0" prefWidth="69.0" />
                              <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleOpenProject" text="Open Existing Project">
                                 <font>
                                    <Font name="System Bold" size="16.0" />
                                 </font>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
